{
    "name": "binalogue/nova-spotify-auth-tool",
    "description": "A Spotify auth tool to use in your Laravel Nova apps.",
    "keywords": [
        "laravel",
        "nova",
        "spotify",
        "package"
    ],
    "homepage": "https://github.com/binalogue/nova-spotify-auth-tool",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Mikel Goig",
            "email": "m@mikelgoig.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Binalogue\\SpotifyAuthTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Binalogue\\SpotifyAuthTool\\SpotifyAuthToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
